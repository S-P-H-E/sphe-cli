{
  "name": "sphe-cli",
  "version": "1.0.44",
  "description": "My personal project scaffolding CLI",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "sphe-cli": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "keywords": [
    "scaffold",
    "next.js",
    "node",
    "cli"
  ],
  "author": "Sphe",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.6.2",
    "chalk-animation": "^2.0.3",
    "execa": "^9.6.1",
    "figlet": "^1.9.3",
    "fs-extra": "^11.3.2",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.9.6",
    "nanospinner": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
